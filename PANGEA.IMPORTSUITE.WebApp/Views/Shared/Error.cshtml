@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

@*<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">An error occurred while processing your request.</h2>*@
<div class="container-fluid text-center">
    <h1 class="error-title">500</h1>
    <h6 class="text-semibold content-group">Oops, an error has occurred. Internal server error!</h6>

    <div class="row">
        <div class="col-lg-4 col-lg-offset-4 col-sm-6 col-sm-offset-3">
            <form action="#" class="main-search">
                <div class="row">
                    <div class="col-sm-6">
                        <a href="/Home/Index" class="btn btn-primary btn-block content-group"><i class="icon-circle-left2 position-left"></i> Go to dashboard</a>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

